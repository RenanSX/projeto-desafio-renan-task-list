{
  "name": "desafio-renan-sxavier-frontend20240522",
  "version": "1.0.0",
  "description": "Frontend para o projeto Todo List",
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rm -rf coverage dist && rm -rf node_modules",
    "dev": "vite",
    "format": "prettier --write ./**/*.{js,jsx,ts,tsx}",
    "lint": "eslint . --fix && prettier --write .",
    "lint-staged-husky": "lint-staged",
    "preview": "vite preview",
    "start": "serve -s dist",
    "test": "jest --coverage --passWithNoTests --silent",
    "update-dependencies": "npx npm-check-updates -u",
    "docs:init": "docsify init ./docs",
    "docs:serve": "docsify serve docs --port 8000"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.2",
    "buffer": "^6.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-saga": "^1.3.0",
    "sass": "^1.77.2",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@testing-library/jest-dom": "^6.4.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@types/react-redux": "^7.1.33",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-react": "^4.3.0",
    "commitizen": "^4.3.0",
    "docsify-cli": "^4.4.4",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "serve": "^14.2.3",
    "ts-jest": "^29.1.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  }
}
